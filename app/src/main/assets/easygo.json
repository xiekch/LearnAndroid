{
  "easyGoVersion": "1.0",
  "client": "com.example.myapplication",
  "logicEntities": [
    {
      "head": {
        "function": "magicwindow",
        "required": "true"
      },
      "body": {
        "mode": "1",
        "activityPairs": [
          {
            "from": "com.example.myapplication.MainActivity",
            "to": "*"
          }
        ],
        "Activities": [
          {
            "name": "com.example.myapplication.ui.UIIndexActivity",
            "defaultFullScreen": "true"
          }
        ],
        "UX": {
          "supportRotationUxCompat": "true",
          "isDraggable": "false"
        }
      }
    }
  ]
}